{
    "extends": "google",
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "object-curly-spacing": "off"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    }
}
